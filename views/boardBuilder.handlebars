<!DOCTYPE html>
<html lang="en">
  {{> boardMeta}}
 <body>
     <button id="ready">are you ready</button>
     <div id="customBoardProps">
        <div id="name">{{#if name }}{{ name }}{{else}}rideBuni{{/if}}</div>
        <div id="contour">{{#if contour }}{{ contour }}{{else}}concave{{/if}}</div>
        <div id="deckColor">{{#if deckColor }}{{ deckColor }}{{else}}EFF2F2{{/if}}</div>
        <div id="bottomColor">{{#if bottomColor }}{{ bottomColor }}{{else}}EFF2F2{{/if}}</div>
        <div id="model">{{#if model }}{{ model }}{{else}}thegem{{/if}}</div>
        <div id="height">{{#if height }}{{ height }}{{else}}5'2{{/if}}</div>
        <div id="width">{{#if width }}{{ width }}{{else}}20.5{{/if}}</div>
        <div id="thickness">{{#if thickness }}{{ thickness }}{{else}}2.5{{/if}}</div>
        <div id="fins">{{#if fins }}{{ fins }}{{else}}twin-fin{{/if}}</div>
        <div id="finish">{{#if finish }}{{ finish }}{{else}}standard{{/if}}</div>
        <div id="rocker">{{#if rocker }}{{ rocker }}{{else}}less-rocker{{/if}}</div>
     </div>
  <div class="customizer" id="custom">
    <h1 id="boardName">{{ boardModel }}</h1>
    <ul class="propList">
        <li class="properties">
            <a href="#" class="property" id="dims">Dimensions</a>
        </li>
        <li class="properties">
            <a href="#" class="property" id="fins">Fins</a>
        </li>
        <li class="properties">
            <a href="#" class="property" id="contours">Contours</a>
        </li>
        <li class="properties">
            <a href="#" class="property" id="rocker">Rocker</a>
        </li>
        <li class="properties">
            <a href="#" class="property" id="colors">Colors</a>
        </li>
        <li class="properties">
            <a href="#" class="property" id="finish">Finish</a>
        </li>
    </ul>
    <span class="drag-notice" id="js-drag-notice">Drag to rotate 360&#176;</span>
    <!-- The canvas element is used to draw the 3D scene -->
    <canvas id="boardCanvas"></canvas>
  </div> 

<div class="test">

</div>

<div class="bottomButtons">
    <button class="saveBoard" onclick="showModal()"><img src="/_possible_photos/download-button.png" alt="download button"></button>
    <form action="/">
    <button type="submit" class="homeButton"><img src="/photos/home.png" alt="home button"></button>
    </form>
</div>
{{>saveBoard}}
</body>
</html>